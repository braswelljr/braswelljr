    "button.secondaryBackground": "#FFFFFF0F",
    "button.secondaryForeground": "#cccccc",
    "button.secondaryHoverBackground": "#ffffff15",
    "checkbox.background": "#313131",
    "checkbox.border": "#ffffff1f",
    "debugToolBar.background": "#181818",
